{
    "setVariable": {
        "scope": "tig",
        "prefix": "set",
        "body": "set ${1|diff-options,blame-options,log-options,main-options,reference-format,line-graphics,truncation-delimiter,horizontal-scroll,git-colors,show-notes,show-changes,vertical-split,split-view-height,split-view-width,status-show-untracked-dirs,status-show-untracked-files,tab-size,diff-context,diff-highlight,ignore-space,commit-order,ignore-case,mailmap,wrap-lines,focus-child,editor-line-number,history-size,mouse,mouse-scroll,mouse-wheel-cursor,refresh-mode,refresh-interval,file-args,rev-args|} = $3",
        "description": "Sets variable for tig"
    },
    "setView": {
        "scope": "tig",
        "prefix": "set",
        "body": "set ${1|blob-view,diff-view,log-view,pager-view,stage-view,blame-view,grep-view,main-view,refs-view,stash-view,status-view,tree-view,author,commit-title,date,file-name,file-size,id,line-number,mode,ref,status,text|} = $3",
        "description": "Set defines view setting of specific key"
    },
    "bind": {
        "scope": "tig",
        "prefix": "bind",
        "body": "bind ${1|main,diff,log,tree,blob,blame,refs,status,stage,stash,pager,help|} $2 $3",
        "description": "Bind defines behavior of user input in specific mode"
    },
    "color": {
        "scope": "tig",
        "prefix": "color",
        "body": "color ${1|default,cursor,status,title-focus,title-blur,search-result,delimiter,header,line-number,id,date,author,mode,overflow,directory,file,file-size|} $2 $3",
        "description": "Bind defines color configurations"
    },
    "source": {
        "scope": "tig",
        "prefix": "source",
        "body": "source $1",
        "description": "Specify path to your files"
    }
}